{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "## Purpose - Create a reusable template with basic functions that can be used to quickly launch new projects",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack  --env mode=production",
    "build:dev": "webpack  --env mode=development",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test": "echo \"No tests specified\"",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "prepare": "husky install",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "postinstall": "node ./scripts/clear-cache.js",
    "remove-feature": "ts-node ./scripts/remove-feature.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/plugin-transform-typescript": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "mini-css-extract-plugin": "^2.9.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.14.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/register": "^7.25.9",
    "@types/babel__core": "^7.20.5",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^8.0.0",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
